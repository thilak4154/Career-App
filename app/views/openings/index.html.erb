<h1>Openings</h1>

<table border="2" width="100%">
  <tr>
    <th width="10%"></th>
    <th width="10%"></th>
    <th width="20%">Technology</th>
    <th width="20%">Skillset</th>
    <th width="20%">Jobtitle</th>
    <th width="20%">Experience</th>
  </tr>

  <% @openings.each do |opening| %>
    <tr align="center">
      <td><%= link_to "Edit" %></td>
      <td><%= link_to "Delete" %></td>
      <td><%= opening.technology %></td>
      <td><%= opening.skillset %></td>
      <td><%= opening.jobtitle %></td>
      <td><%= opening.experience %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Opening', new_opening_path %>
